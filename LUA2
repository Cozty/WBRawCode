local player = game.Players
local TS = game:GetService("TweenService")
local RS = game:GetService("RunService")
local TI1 = TweenInfo.new(1)
local TI2 = TweenInfo.new(.5)
--time to actually start this.
local StartFrameVis = TS:Create(StartMainFrame,TI1,{BackgroundTransparency=0})
local StartTopPanelVis = TS:Create(StartTopPanel,TI1,{BackgroundTransparency=0})
local StartButtonVis = TS:Create(StartButton,TI1,{BackgroundTransparency=0})
local StartTextButtonVis = TS:Create(StartButton,TI1,{TextTransparency=0})
local StartTitleButtonVis = TS:Create(StartTitle,TI1,{TextTransparency=0})
local StartCloseTextVis = TS:Create(StartClose,TI1,{TextTransparency=0})
local started = false
print("bruh")
StartTopPanelVis:Play()
StartFrameVis:Play()
StartButtonVis:Play()
StartTextButtonVis:Play()
StartTitleButtonVis:Play()
StartCloseTextVis:Play()

StartMainFrame.Draggable = true
StartMainFrame.Active = true

--color
spawn(function()
	local colourWheel = ColourWheel
	local wheelPicker = Picker

	local darknessPicker = DarknessPicker
	local darknessSlider = Slider




	local uis = game:GetService("UserInputService")


	local buttonDown = false
	local movingSlider = false


	local function updateColour(centreOfWheel)


		local colourPickerCentre = Vector2.new(
			colourWheel.Picker.AbsolutePosition.X + (colourWheel.Picker.AbsoluteSize.X/2),
			colourWheel.Picker.AbsolutePosition.Y + (colourWheel.Picker.AbsoluteSize.Y/2)
		)
		local h = (math.pi - math.atan2(colourPickerCentre.Y - centreOfWheel.Y, colourPickerCentre.X - centreOfWheel.X)) / (math.pi * 2)

		local s = (centreOfWheel - colourPickerCentre).Magnitude / (colourWheel.AbsoluteSize.X/2)

		local v = math.abs((darknessSlider.AbsolutePosition.Y - darknessPicker.AbsolutePosition.Y) / darknessPicker.AbsoluteSize.Y - 1)


		local hsv = Color3.fromHSV(math.clamp(h, 0, 1), math.clamp(s, 0, 1), math.clamp(v, 0, 1))


		Beast.TextColor3 = hsv
		Wind.TextColor3 = hsv
		Moon.TextColor3 = hsv
		Water.TextColor3 = hsv
		Flame.TextColor3 = hsv
		Sound.TextColor3 = hsv
		Love.TextColor3 = hsv
		Serpent.TextColor3 = hsv
		Mist.TextColor3 = hsv
		Insect.TextColor3 = hsv
		Stone.TextColor3 = hsv
		Thunder.TextColor3 = hsv
		ColorClose.TextColor3 = hsv
		ColorTitle.TextColor3 = hsv
		MainTitle.TextColor3 = hsv
		MainClose.TextColor3 = hsv
		Reset.TextColor3 = hsv
		Settings.TextColor3 = hsv
		darknessPicker.UIGradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0, hsv), 
			ColorSequenceKeypoint.new(1, Color3.new(0, 0, 0))
		}
	end


	colourWheel.MouseButton1Down:Connect(function()
		buttonDown = true
	end)

	darknessPicker.MouseButton1Down:Connect(function()
		movingSlider = true
	end)


	uis.InputEnded:Connect(function(input)

		if input.UserInputType ~= Enum.UserInputType.MouseButton1 then return end

		buttonDown = false
		movingSlider = false
	end)


	uis.InputChanged:Connect(function(input)

		if input.UserInputType ~= Enum.UserInputType.MouseMovement then return end


		local mousePos = uis:GetMouseLocation() - Vector2.new(0, game:GetService("GuiService"):GetGuiInset().Y)

		local centreOfWheel = Vector2.new(colourWheel.AbsolutePosition.X + (colourWheel.AbsoluteSize.X/2), colourWheel.AbsolutePosition.Y + (colourWheel.AbsoluteSize.Y/2))

		local distanceFromWheel = (mousePos - centreOfWheel).Magnitude


		if distanceFromWheel <= colourWheel.AbsoluteSize.X/2 and buttonDown then

			wheelPicker.Position = UDim2.new(0, mousePos.X - colourWheel.AbsolutePosition.X, 0, mousePos.Y - colourWheel.AbsolutePosition.Y)


		elseif movingSlider then

			darknessSlider.Position = UDim2.new(darknessSlider.Position.X.Scale, 0, 0, 
				math.clamp(
					mousePos.Y - darknessPicker.AbsolutePosition.Y, 
					0, 
					darknessPicker.AbsoluteSize.Y)
			)	
		end


		updateColour(centreOfWheel)
	end)
end)


local DarkBlue = TS:Create(StartButton,TI2, {TextColor3=Color3.fromRGB(75, 114, 159)})
local BabyBlue = TS:Create(StartButton,TI2, {TextColor3=Color3.fromRGB(121, 182, 255)})

spawn(function()
	while true do
		wait(.5)
		DarkBlue:Play()
		wait(.5)
		BabyBlue:Play()
		RS.Stepped:Wait()
	end
end)

StartClose.MouseButton1Click:Connect(function()
	local StartFrameInVis = TS:Create(StartMainFrame,TI1,{BackgroundTransparency=1})
	local StartTopPanelInVis = TS:Create(StartTopPanel,TI1,{BackgroundTransparency=1})
	local StartButtonInVis = TS:Create(StartButton,TI1,{BackgroundTransparency=1})
	local StartTextButtonInVis = TS:Create(StartButton,TI1,{TextTransparency=1})
	local StartTitleButtonInVis = TS:Create(StartTitle,TI1,{TextTransparency=1})
	local StartCloseTextInVis = TS:Create(StartClose,TI1,{TextTransparency=1})
	StartTopPanelInVis:Play()
	StartFrameInVis:Play()
	StartButtonInVis:Play()
	StartTextButtonInVis:Play()
	StartTitleButtonInVis:Play()
	StartCloseTextInVis:Play()
	wait(1.2)
	game.Debris:AddItem(WisteriaBreaths,0.1)
end)

StartButton.MouseButton1Click:Connect(function()
	local StartFrameInVis = TS:Create(StartMainFrame,TI1,{BackgroundTransparency=1})
	local StartTopPanelInVis = TS:Create(StartTopPanel,TI1,{BackgroundTransparency=1})
	local StartButtonInVis = TS:Create(StartButton,TI1,{BackgroundTransparency=1})
	local StartTextButtonInVis = TS:Create(StartButton,TI1,{TextTransparency=1})
	local StartTitleButtonInVis = TS:Create(StartTitle,TI1,{TextTransparency=1})
	local StartCloseTextInVis = TS:Create(StartClose,TI1,{TextTransparency=1})
	StartTopPanelInVis:Play()
	StartFrameInVis:Play()
	StartButtonInVis:Play()
	StartTextButtonInVis:Play()
	StartTitleButtonInVis:Play()
	StartCloseTextInVis:Play()
	wait(1.5)
	MainFrame.Visible = true
	started = true
end)

MainFrame.Draggable = true
MainFrame.Active = true

MainFrame.BackgroundTransparency = 1
TopPanelMain.BackgroundTransparency = 1
ScrollingFrame.BackgroundTransparency = 1
ScrollingFrame.ScrollBarImageTransparency = 1
for i,v in ipairs(MainFrame:GetChildren()) do
	if v.ClassName == "Frame" or v.ClassName == "ScrollingFrame" then

		for i,g in ipairs(v:GetChildren()) do
			if g.ClassName == "TextButton" or g.ClassName == "TextLabel" then
				g.TextTransparency=1
				g.BackgroundTransparency=1
			end
		end
	end
end

RS.Heartbeat:Connect(function()
	if started == true then

		wait(1)
		local FadeIn1 = TS:Create(MainFrame,TI1,{BackgroundTransparency=0})
		FadeIn1:Play()

		for i,v in ipairs(MainFrame:GetChildren()) do
			if v.ClassName == "Frame" or v.ClassName == "ScrollingFrame" then
				local FadeIn = TS:Create(v,TI1, {BackgroundTransparency=0})

				FadeIn:Play()
				for i,g in ipairs(v:GetChildren()) do
					if g.ClassName == "TextButton" or g.ClassName == "TextLabel" then
						local FadeIn2 = TS:Create(g,TI1,{TextTransparency=0},{BackgroundTransparency=0})
						FadeIn2:Play()

					end
				end
			end
		end

		ColorPickerFrame.Draggable = true
		ColorPickerFrame.Active = true

		ColorClose.MouseButton1Click:connect(function()
			ColorPickerFrame.Visible = false
		end)

		Settings.MouseButton1Click:connect(function()
			ColorPickerFrame.Visible = true
			--color wheel stuff

		end)
		
		
		
		
		
		MainClose.MouseButton1Click:Connect(function()
			game.Debris:AddItem(WisteriaBreaths,0.1)
			game.Debris:AddItem(script,0.1)
		end)

		Beast.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Beast")
			Beast.Text = "Checked"
		end)

		Flame.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Flame")
			Flame.Text = "Checked"

		end)

		Water.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Water")
			Water.Text = "Checked!"

		end)

		Sound.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Sound")
			Sound.Text = "Checked!"

		end)

		Mist.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Mist")
			Mist.Text = "Checked!"

		end)

		Moon.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Moon")
			Moon.Text = "Checked!"

		end)

		Insect.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Insect")
			Insect.Text = "Checked!"

		end)

		Wind.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Wind")
			Wind.Text = "Checked!"

		end)

		Love.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Love")
			Love.Text = "Checked!"
		end)

		Thunder.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Thunder")
			Thunder.Text = "Checked!"
		end)

		Serpent.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Serpent")
			Serpent.Text = "Checked!"
		end)

		Stone.MouseButton1Click:connect(function()
			game:GetService("ReplicatedStorage").BattleNPC:FireServer("Stone")
			Stone.Text = "Checked!"
		end)
		
		Reset.MouseButton1Click:Connect(function()
			Stone.Text = "Stone"
			Wind.Text = "Wind"
			Serpent.Text = "Serpent"
			Love.Text = "Love"
			Moon.Text = "Moon"
			Sound.Text = "Sound"
			Flame.Text = "Flame"
			Water.Text = "Water"
			Insect.Text = "Insect"
			Beast.Text = "Beast"
			Mist.Text = "Mist"
			Thunder.Text = "Thunder"
		end)



	end

end)
