Umbrella.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Workspace:GetChildren()) do
		if v.Name == "Handle" and v.ClassName == "MeshPart" then
			if v.MeshId == "rbxassetid://412771199" then
				print("UMBRELLA") 
				fireclickdetector(v.ClickDetector)
			end
		end
	end
end)

Sleeve1.MouseButton1Click:Connect(function()
	local Parts = {}
	for _,v in ipairs(game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name):GetChildren()) do
		if v.Name == "Haoripart" and v.MeshId == "rbxassetid://6009313450" then
			table.insert(Parts,v)
			local sleeve, sleeve2 = table.unpack(Parts)
			print(sleeve)
			for i, b in pairs(sleeve:GetChildren()) do
				b:Destroy()
			end
		end
	end
end)
NoHaori.MouseButton1Click:Connect(function()
	for _, v in pairs(game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name):GetChildren()) do 
		if v.Name == "Haoripart" then
			v.ManualWeld:Destroy()
		end
	end
end)
NoHorns.MouseButton1Click:Connect(function()
	game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name).Horns:Destroy()
end)
Sleeveless.MouseButton1Click:Connect(function()
	for _,v in ipairs(game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name):GetChildren()) do
		if v.Name == "Haoripart" and v.MeshId == "rbxassetid://6009313450" then
			v.ManualWeld:Destroy()
		end
	end
end)
LowHPHaori.MouseButton1Click:Connect(function()
	game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name).Health.Changed:Connect(function(Val)
		print(Val)
		if Val < 222 then
			for _, v in pairs(game.Workspace.Living:FindFirstChild(game.Players.LocalPlayer.Name):GetChildren()) do 
				if v.Name == "Haoripart" then
					v.ManualWeld:Destroy()
				end
			end
		end

	end)
end)

