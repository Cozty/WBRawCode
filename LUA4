local GUITOGGLE = false
local uis = game:GetService("UserInputService")

uis.InputBegan:Connect(function(key, chatting)
	if not chatting and key.KeyCode == Enum.KeyCode.N then
		if GUITOGGLE == false then
			MainFrame.Visible = false
			GUITOGGLE = true
		else
			MainFrame.Visible = true
			GUITOGGLE = false
		end
	end
end)
--varibles
wait(1)
local player = game.Players.LocalPlayer
local TS = game:GetService("TweenService")
local RS = game:GetService("RunService")
local TI1 = TweenInfo.new(1) 
local TI2 = TweenInfo.new(.5)
local SelectorTab = false
local MiscToggle = false
local FoodToggle = false
local BreathToggle = false
local CustomToggle = false
--animations
print("SADASD")
local SelectTweenIN = TS:Create(SelectFrame, TI1, {Position=UDim2.new(-0.014, 0,0.118, 0)}) 
local SelectTweenOUT = TS:Create(SelectFrame, TI1, {Position=UDim2.new(-0.309, 0,0.121, 0)})

local SelectorButtonRotated = TS:Create(SelectButton,TI2, {Rotation=45})
local SelectorButtonNormal = TS:Create(SelectButton,TI2, {Rotation=0})

local InFrameMisc = TS:Create(MiscFrame,TI1, {Position=UDim2.new(0.32, 0,0.14, 0)})
local OutFrameMisc = TS:Create(MiscFrame,TI1, {Position=UDim2.new(1.011, 0,0.14, 0)})

local InFrameFood = TS:Create(FoodFrame,TI1, {Position=UDim2.new(0.32, 0,0.14, 0)})
local OutFrameFood = TS:Create(FoodFrame,TI1, {Position=UDim2.new(1.011, 0,0.14, 0)})

local InFrameBreath = TS:Create(BreathingFrame,TI1, {Position=UDim2.new(0.32, 0,0.14, 0)})
local OutFrameBreath = TS:Create(BreathingFrame,TI1, {Position=UDim2.new(1.011, 0,0.14, 0)})

local InFrameCustom = TS:Create(CustomFrame,TI1, {Position=UDim2.new(0.32, 0,0.14, 0)})
local OutFrameCustom = TS:Create(CustomFrame,TI1, {Position=UDim2.new(1.011, 0,0.14, 0)})
--functions
print("BASDWAEA")
local function ResetCustomFrame()
	CustomFrame.Position = UDim2.new(-0.67, 0,0.14, 0)
end
local function ResetMiscFrame()
	MiscFrame.Position = UDim2.new(-0.67, 0,0.14, 0)
end
local function ResetBreathingFrame()
	BreathingFrame.Position = UDim2.new(-0.67, 0,0.14, 0)
end
local function ResetFoodFrame()
	FoodFrame.Position = UDim2.new(-0.67, 0,0.14, 0)
end

SelectButton.MouseButton1Click:Connect(function()
	print("BRUASDASRF")
	if SelectorTab == false then
		print("BRUHWEOKRKR")
		SelectorTab = true
		SelectTweenIN:Play()
		SelectorButtonRotated:Play()
	else
		SelectTweenOUT:Play()
		SelectorTab = false
		SelectorButtonNormal:Play()
	end
end)

close.MouseButton1Click:Connect(function()
	game.Debris:AddItem(CozGUI,0.1)
end)

SelectMisc.MouseButton1Click:Connect(function()
	InFrameMisc:Play()
	MiscToggle = true
	if BreathToggle == true then
		OutFrameBreath:Play()
		wait(1.1)
		ResetBreathingFrame()
		BreathToggle = false
	end
	if FoodToggle == true then
		OutFrameFood:Play()
		wait(1.1)
		ResetFoodFrame()
		FoodToggle = false
	end
	if CustomToggle == true then
		OutFrameCustom:Play()
		wait(1.1)
		ResetCustomFrame()
		CustomToggle = false
	end

end)

SelectBreathing.MouseButton1Click:Connect(function()
	InFrameBreath:Play()
	BreathToggle = true
	if FoodToggle == true then
		OutFrameFood:Play()
		wait(1.1)
		ResetFoodFrame()
		FoodToggle = false
	end
	if CustomToggle == true then
		OutFrameCustom:Play()
		wait(1.1)
		ResetCustomFrame()
		CustomToggle = false
	end
	if MiscToggle == true then
		OutFrameMisc:Play()
		wait(1.1)
		ResetMiscFrame() 
		MiscToggle = false
	end

end)

