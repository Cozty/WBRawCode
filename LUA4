Cloth1.MouseButton1Click:connect(function()
	for i,v in pairs(game.Workspace:FindFirstChild("Demon Outfits"):GetChildren()) do
		--print(v)
		for _, m in pairs(v:GetDescendants()) do

			if m.ClassName == "Shirt" then
				local shirtid = m.ShirtTemplate
				--print(shirtid)
				if shirtid == "http://www.roblox.com/asset/?id=5187872985" then
					--print("Found it!!")
					print(m.Parent)
					--print(m.Parent.HumanoidRootPart.Name)
					fireclickdetector(m.Parent.HumanoidRootPart.ClickDetector)
				end
			end
		end
	end
end)

